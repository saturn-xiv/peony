toolchain=/opt/toolchains/sysroot-glibc-linaro-2.25-2019.12-arm-linux-gnueabihf
target_host=/opt/toolchains/gcc-linaro-7.5.0-2019.12-x86_64_arm-linux-gnueabihf/bin/arm-linux-gnueabihf

[settings]
os=Linux
os_build=Linux
arch=armv7hf
arch_build=x86_64
compiler=gcc
compiler.cppstd=17
compiler.version=7.5
compiler.libcxx=libstdc++11
build_type=Release

[options]

[build_requires]

[env]
CONAN_CMAKE_FIND_ROOT_PATH=$toolchain
CONAN_CMAKE_SYSROOT=$toolchain
CHOST=$target_host
USE_DEV_URANDOM=1
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
CC=$target_host-gcc
CXX=$target_host-g++
STRIP=$target_host-strip
LD=$target_host-ld
#RC=$target_host-windres
