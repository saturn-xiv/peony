target_host=arm-linux-gnueabihf

[settings]
os=Linux
os_build=Linux
arch=armv7hf
arch_build=x86_64
compiler=gcc
compiler.version=9
compiler.libcxx=libstdc++11
build_type=Release

[options]

[build_requires]

[env]
CONAN_CMAKE_FIND_ROOT_PATH=$root_fs
PATH=[ $toolchain_root/bin ]
CHOST=$target_host
USE_DEV_URANDOM=1
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
STRIP=$target_host-strip
LD=$target_host-ld
CC=$target_host-gcc
CXX=$target_host-g++